{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "db_init": "npx tsx ./scripts/dbSetup.ts",
    "db_push": "npx drizzle-kit push",
    "db_generate": "npx drizzle-kit generate",
    "db_migrate": "npx drizzle-kit migrate",
    "db_delete": "npx tsx ./scripts/dbSetup.ts DELETE",
    "db_nuke": "npx tsx ./scripts/dbSetup.ts DELETE && rm -rf src/database/migrations"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@tailwindcss/vite": "^4.1.11",
    "better-sqlite3": "^11.9.1",
    "bootstrap": "^5.3.5",
    "dotenv-expand": "^12.0.2",
    "drizzle-orm": "^0.41.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.5.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^3.1.2",
    "autoprefixer": "^10.4.21",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.30.6",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "tailwindcss": "3",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^3.1.2"
  },
  "main": "dist-electron/main.js"
}
